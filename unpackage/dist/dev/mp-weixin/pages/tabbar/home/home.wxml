<view bind:__l="__l"><view><swiper class="swiper-image" autoplay="true" indicator-dots="true" interval="5000" duration="300" circular="true"><block wx:for="{{imageList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><image style="width:100%;height:100%;" src="{{item}}"></image></swiper-item></block></swiper></view><view><uni-notice-bar show-icon="true" scrollable="true" single="true" text="{{nartitle.storeName+'提交'+nartitle.ruleTitle+'活动成功'}}"></uni-notice-bar></view><view class="uni-list"><block wx:for="{{reslutList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{index<=2}}"><view class="uni-list-cell" hover-class="uni-list-cell-hover" data-event-opts="{{[['tap',[['jumpDetail',[item.rulePublishId]]]]]}}" bindtap="__e"><view class="uni-media-list"><view class="uni-media-list-body"><view class="uni-media-list-text-top">{{item.ruleTitle+''}}</view><view class="uni-media-list-text-bottom ">{{'截止时间:'+item.rulePublishDatetime.ruleDatetimeEnd+''}}<block wx:if="{{item.isAlive==1}}"><label>抢单中</label></block><block wx:else><label>活动结束</label></block></view></view></view></view></block></block></view><block wx:for="{{reslutList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{index<=2}}"><view class="uni-list-cell" hover-class="uni-list-cell-hover" data-event-opts="{{[['tap',[['jumpDetail',[item.rulePublishId]]]]]}}" bindtap="__e"><view><view class="item-title">{{item.ruleTitle+''}}</view><view class="item-text">{{'截止时间:'+item.rulePublishDatetime.ruleDatetimeEnd+''}}<block wx:if="{{item.isAlive==1}}"><label>抢单中</label></block><block wx:else><label>活动结束</label></block></view></view></view></block></block></view>